<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.control.Tab" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="nl.fxtooly.tab.queryexecutor.QueryExecutorController">
  <content>
    <BorderPane fx:id="content" prefHeight="353.0" prefWidth="630.0">
      <bottom>
        <TableView fx:id="results" prefHeight="250.0" prefWidth="-1.0" />
      </bottom>
      <center>
        <SplitPane dividerPositions="0.5" focusTraversable="true" maxHeight="-1.0" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0">
          <items>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
              <children>
                <TextArea fx:id="query" prefHeight="158.0" prefWidth="552.0" wrapText="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
              </children>
            </AnchorPane>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
              <children>
                <Button fx:id="execute" layoutX="10.0" layoutY="135.0" mnemonicParsing="false" onAction="#execute" text="Execute" />
                <TextField layoutX="10.0" layoutY="33.0" prefWidth="175.0" />
                <Button layoutX="111.0" layoutY="60.0" mnemonicParsing="false" text="Save Query" />
              </children>
            </AnchorPane>
          </items>
        </SplitPane>
      </center>
      <left>
        <TreeView fx:id="prevQueries" maxHeight="-1.0" minHeight="-1.0" prefHeight="150.0" />
      </left>
    </BorderPane>
  </content>
</fx:root>
